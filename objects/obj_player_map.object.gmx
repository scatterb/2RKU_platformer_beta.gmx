            <string>/// Movement


// Sets the analog stick of a gamepad, else the player will move by it's own
gamepad_set_axis_deadzone(0, 0.7);


// Move down
if ((keyboard_check(vk_down)) || (gamepad_axis_value(0, gp_axislv) &gt; 0) || (gamepad_button_check(0,gp_padd))) &amp;&amp; (place_free(x, y+2))  { // place_free checks only against solid objects: http://gmc.yoyogames.com/index.php?showtopic=481823
    y += 8;
    //sprite_index = spr_walkdown;
    image_speed = 0.2;
    Direction = "Down";
}
// Move up
if ((keyboard_check(vk_up)) || (gamepad_axis_value(0, gp_axislv) &lt; 0) || (gamepad_button_check(0,gp_padu))) &amp;&amp; (place_free(x, y-2))  {
    y -= 8;
    //sprite_index = spr_walkup;
    image_speed = 0.2;
    Direction = "Up";
}
// Move left
if ((keyboard_check(vk_left)) || (gamepad_axis_value(0, gp_axislh) &lt; 0)  || gamepad_button_check(0, gp_padl)) &amp;&amp; (place_free(x-2, y))  {
    x -= 8;
    //sprite_index = spr_walkleft;
    image_speed = 0.2;
    Direction = "Left";
}
// Move right
if ((keyboard_check(vk_right)) || (gamepad_axis_value(0, gp_axislh) &gt; 0) || (gamepad_button_check(0,gp_padr))) &amp;&amp; (place_free(x+2, y))  {
    x += 8;
    //sprite_index = spr_walkright;
    image_speed = 0.2;
    Direction = "Right";
}


// Toggle fullscreen
if keyboard_check_pressed(vk_escape) || (gamepad_button_check_pressed(0, gp_select))
{
   if window_get_fullscreen()
   {
      window_set_fullscreen(false);
   }
   else
   {
      window_set_fullscreen(true);
   }
}

</string>
